{
  "env": {
    "node": true,
    "es2022": true,
    "jest": true,
    "serviceworker": false,
    "jquery": false
  },
  "extends": [ "eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-var": "error",
    "prefer-const": "error",
  },
   "ignorePatterns": [
    "node_modules/",
    "dist/"
  ]
}